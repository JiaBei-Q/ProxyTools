#!name = Google重定向
#!desc = 将google.cn/google.com.hk重定向回google.com
#!icon = https://raw.githubusercontent.com/mphin/ProxyTools/main/plugin/Google/icon/Google.png
#!date = 2024-03-17 10:51:34

[Rewrite]
^https?:\/\/(www.)?(g|google)\.cn https://www.google.com 302
^https?:\/\/(www.)?(g|google)\.com.hk https://www.google.com 302

[Mitm]
hostname = *.google.cn, *.google.com.hk
